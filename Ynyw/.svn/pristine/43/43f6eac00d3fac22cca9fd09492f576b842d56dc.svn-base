package com.huawei.ynyw.dto.common;

import com.huawei.smartcare.dac.sdk.bean.ColumnType;
import com.huawei.smartcare.dac.sdk.bean.Style;

/***
 * 文件导出的实体类
 * 
 * @author Administrator
 */
public class FieldMap
{
    private String cname;
    private String ename;
    private ColumnType colomType;
    private Style style;
    
    /***
     * 构造方法
     * 
     * @param cname
     *            中文
     * @param ename
     *            英文
     * @param colomType
     *            列类型
     * @param style
     *            格式
     */
    public FieldMap(String cname, String ename, ColumnType colomType, Style style)
    {
        super();
        this.cname = cname;
        this.ename = ename;
        this.colomType = colomType;
        this.style = style;
    }
    
    public String getCname()
    {
        return cname;
    }
    
    public void setCname(String cname)
    {
        this.cname = cname;
    }
    
    public String getEname()
    {
        return ename;
    }
    
    public void setEname(String ename)
    {
        this.ename = ename;
    }
    
    public ColumnType getColomType()
    {
        return colomType;
    }
    
    public void setColomType(ColumnType colomType)
    {
        this.colomType = colomType;
    }
    
    public Style getStyle()
    {
        return style;
    }
    
    public void setStyle(Style style)
    {
        this.style = style;
    }
    
}
