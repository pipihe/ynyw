package com.huawei.ynyw.dto;

import java.util.Arrays;
import java.util.List;

public class SweetPage {
	private Integer size;
	private Integer total;
	private Integer start;
	private Integer limit;
	private int[] select;
	private Integer[] filter;
	private List<SweetOrder> order;

	public SweetPage() {
		this.select = new int[] {5};
	}

	public Integer getStart() {
		return this.start;
	}

	public void setStart(Integer start) {
		this.start = start;
	}

	public Integer getLimit() {
		return this.limit;
	}

	public void setLimit(Integer limit) {
		this.limit = limit;
	}

	public Integer[] getFilter() {
		return this.filter;
	}

	public void setFilter(Integer[] filter) {
		this.filter = filter;
	}

	public Integer getSize() {
		return this.size;
	}

	public void setSize(Integer size) {
		this.size = size;
	}

	public Integer getTotal() {
		return this.total;
	}

	public void setTotal(Integer total) {
		this.total = total;
	}

	public int[] getSelect() {
		return this.select;
	}

	public void setSelect(int[] select) {
		this.select = select;
	}

	public List<SweetOrder> getOrder() {
		return this.order;
	}

	public void setOrder(List<SweetOrder> order) {
		this.order = order;
	}

	public String toString() {
		return "SweetPage [size=" + this.size + ", total=" + this.total + ", start=" + this.start + ", limit="
				+ this.limit + ", filter=" + Arrays.toString(this.filter) + ", order=" + this.order + ", select="
				+ Arrays.toString(this.select) + "]";
	}
}