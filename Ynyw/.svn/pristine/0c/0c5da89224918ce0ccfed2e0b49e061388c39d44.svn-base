.hearder_analyse{
	width: 100%;
	height:15%;
	background-color: #FFF;
	margin-bottom: 20px;
}
.main{
	width: calc(100% - 100px);
	margin: 0 auto;
	height:calc(87% - 50px);
	background-color: #fff;
}
/*自有业务分析页面样式*/
.hearder_com{
	width: 100%;
	height: 40px;
	position: relative;
	top:13%;
}
.drop_word3 {
    width: 42px;
    line-height: 40px;
    display: block;
    color: #2e2e2e;
    font-size: 14px;
    float: left;
}
.analyse_time{
	width: 38%;
    height: 100%;
    float: left;
}
.analyse_time2{
	width: 26%;
    height: 100%;
    float: left;
}
.analyse_time3{
	width: 14%;
	margin-left: 22px;
    height: 100%;
    float: left;
}
.drop3_time{
	width: 80%;
    height: 100%;
    float: left;
}
.analyse_kuang{
	width: 44%;
    height: 70%;
    border: #acb3bd solid 1px;
    margin-top: 7px;
    margin-left: 5px;
    float: left;
}
.analyse_kuang2{
	width: 33%;
    height: 70%;
    border: #acb3bd solid 1px;
    margin-top: 7px;
    margin-left: 5px;
    float: left;
}
.analyse_kuang3{
	width: 43%;
    height: 70%;
    border: #acb3bd solid 1px;
    margin-top: 7px;
    margin-left: 5px;
    float: left;
}
.fkx{
	width: 4px;
    height: 70%;
    margin-top: 7px;
    margin:8px 4px;
    float: left;
}
.analyse_btn{
	width: 5.7%;
    height: 100%;
    float: right;
    margin-left: 30px;
}
.analyse_btn span{
	width: 100%;
	height: 30px;
	display: inline-block;
	background-color:#2a87ff;
	border-radius: 6px;
    text-align: center;
    line-height: 30px;
    margin-top: 3px;
    color: #fff;
}
.allannv{
	width: 17px;
	height: 17px;
	float: left;
    top: calc(50% - 9px);
    position: absolute;   
	background: url(../images/annvlight.png) 0 center no-repeat;
	display: block;
}
.hearder_bot a:active{
	background: url(../images/annv.png) 0 center no-repeat;
}
/*头部左侧时间单选样式*/
.hearder_left{
	width:25%;
	height:100%;
	padding-left: 5%;
	float: left;
	font-size: 14px;
}
.hearder_left ul{
	width:100%;
}
.hearder_left li{
	width: 20%;
	height: 100%;
	float: left;
	line-height: 40px;
}
.hearder_left li span{
	width: 18px;
    height: 18px;
    background-image: url(../images/icon1.png);
    display: inline-block;
    vertical-align: middle;
    background-size: contain;
    margin-top: -3px;
    margin-right: 3px;
}
.hearder_left ul li:first-child span{
	   background-image: url(../images/icon2.png);
}
/*头部右侧搜索框样式start*/
.hearder_right{
	width:65%;
	height:100%;
	float: left;
}  
.hearder_bot{
	width:93%;
	height:100%;
	float: left;
	padding-left: 5%;
}
/*头部右侧搜索框样式end*/
/*头部单选按钮start*/
.main_chart{
	width:calc(95% - 30px);
	height: 100%;
	margin: 0 auto;
	padding-top: 30px;
}
.chart_tit{
	width: 100%;
	height: 25px;
	color: #333;
	font-size: 18px;
	padding-left: 15px;
	margin-bottom: 20px;
	border-left: #1177FB solid 3px;
}
.chart_content{
	width: 100%;
	height:60%;
}
/*头部单选按钮end*/
@media screen and (min-width: 1280px) {
.hearder_analyse{
    height: 12%;
}
.hearder_com {
    height: 30px;
    top: 6%;
    margin-bottom: 7px;
}
.analyse_btn span{
	height: 25px;
	font-size: 12px;
	line-height: 25px;
}
.allannv{
	width: 15px;
    height: 15px;
    background-size: contain;
    top: calc(50% - 4px);
}
.drop_word3{
	line-height:34px;
}
.hearder_left li{
	line-height:34px;
}
}
@media screen and (min-width: 1366px) {
.hearder_analyse{
    height: 12%;
}
.hearder_com {
    height: 30px;
    top: 9%;
}
.allannv {
	top: calc(50% - 2px);
}
.analyse_btn span{
	height: 25px;
	font-size: 12px;
	line-height: 25px;
}
.allannv{
	width: 15px;
    height: 15px;
    background-size: contain;
}
.drop_word3{
	line-height:34px;
}
.hearder_left li{
	line-height:34px;
}
}
@media screen and (min-width: 1920px) {
.hearder_analyse{
    height: 11%;
}
.hearder_com {
    height: 40px;
    top: 13%;
}
.allannv {
	top: calc(50% - 9px);
}
.analyse_btn span{
	font-size: 14px;
    height: 30px;
    line-height: 30px;
}
}
